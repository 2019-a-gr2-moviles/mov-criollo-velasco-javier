<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ActualizarActivity">

    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="Name"
            android:ems="10"
            android:id="@+id/txtNombre" android:layout_marginTop="52dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginEnd="96dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="1.0"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="Name"
            android:ems="10"
            tools:layout_editor_absoluteY="148dp" android:id="@+id/txtLiga"
            app:layout_constraintEnd_toEndOf="@+id/txtNombre" app:layout_constraintStart_toStartOf="@+id/txtNombre"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="Name"
            android:ems="10"
            android:id="@+id/txtFechaCreacion" tools:layout_editor_absoluteY="241dp"
            app:layout_constraintEnd_toEndOf="@+id/txtLiga" app:layout_constraintStart_toStartOf="@+id/txtLiga"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="Name"
            android:ems="10"
            tools:layout_editor_absoluteY="328dp" android:id="@+id/txtNumCopInter"
            app:layout_constraintEnd_toEndOf="@+id/txtFechaCreacion" app:layout_constraintStart_toStartOf="@+id/txtFechaCreacion"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="Name"
            android:ems="10"
            tools:layout_editor_absoluteY="417dp" android:id="@+id/txtCampeonAct"
            app:layout_constraintEnd_toEndOf="@+id/txtNumCopInter" app:layout_constraintStart_toStartOf="@+id/txtNumCopInter"/>
    <Button
            android:text="Eliminar"
            android:layout_width="wrap_content"
            android:layout_height="41dp"
            android:id="@+id/btnEliminar"
            app:layout_constraintEnd_toStartOf="@+id/btnMenuRetorno" android:layout_marginEnd="72dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintTop_toBottomOf="@+id/btnActualizar" android:layout_marginTop="12dp"
            app:layout_constraintHorizontal_bias="0.455" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent"/>
    <Button
            android:text="Actualizar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="612dp"
            android:id="@+id/btnActualizar"
            app:layout_constraintEnd_toEndOf="@+id/btnGestionarJugador"
            app:layout_constraintStart_toStartOf="@+id/btnGestionarJugador"/>
    <Button
            android:text="Crear Ingrediente"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="491dp"
            android:id="@+id/btnCrearJugador"
            app:layout_constraintEnd_toEndOf="@+id/txtCampeonAct" app:layout_constraintStart_toStartOf="@+id/txtCampeonAct"/>
    <Button
            android:text="Gestionar Ingredientes"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="556dp"
            android:id="@+id/btnGestionarJugador"
            app:layout_constraintEnd_toEndOf="@+id/btnCrearJugador"
            app:layout_constraintStart_toStartOf="@+id/btnCrearJugador"/>
    <Button
            android:text="Volver"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/btnMenuRetorno"
            android:layout_marginEnd="32dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintVertical_bias="0.992"/>
</android.support.constraint.ConstraintLayout>