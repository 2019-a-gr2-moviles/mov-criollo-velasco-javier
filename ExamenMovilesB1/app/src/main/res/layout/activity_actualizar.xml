<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ActualizarActivity">

    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="Name"
            android:ems="10"
            android:id="@+id/txtNombre2" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="Name"
            android:ems="10"
            android:id="@+id/txtLiga2" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/txtNombre2"
            app:layout_constraintEnd_toEndOf="@+id/txtNombre2" app:layout_constraintStart_toStartOf="@+id/txtNombre2"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="Name"
            android:ems="10"
            android:id="@+id/txtFechaCreacion2" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/txtLiga2"
            app:layout_constraintEnd_toEndOf="@+id/txtLiga2" app:layout_constraintStart_toStartOf="@+id/txtLiga2"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="Name"
            android:ems="10"
            android:id="@+id/txtNumCopInter2" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/txtFechaCreacion2"
            app:layout_constraintEnd_toEndOf="@+id/txtFechaCreacion2"
            app:layout_constraintStart_toStartOf="@+id/txtFechaCreacion2"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="Name"
            android:ems="10"
            android:id="@+id/txtCampeonAct2" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/txtNumCopInter2"
            app:layout_constraintEnd_toEndOf="@+id/txtNumCopInter2"
            app:layout_constraintStart_toStartOf="@+id/txtNumCopInter2"/>
    <Button
            android:text="Button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/btnEliminar2" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/txtCampeonAct2"
            app:layout_constraintEnd_toStartOf="@+id/btnActualizar2"
            android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintHorizontal_bias="0.608"/>
    <Button
            android:text="Button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/btnActualizar2"
            tools:layout_editor_absoluteY="273dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="80dp"/>
    <Button
            android:text="Button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/btnMenuRetorno2"
            app:layout_constraintTop_toBottomOf="@+id/btnGestionarJugador2"
            app:layout_constraintEnd_toEndOf="@+id/btnGestionarJugador2"
            app:layout_constraintStart_toStartOf="@+id/btnGestionarJugador2" android:layout_marginTop="16dp"
            app:layout_constraintHorizontal_bias="0.0"/>
    <Button
            android:text="btnCrearJugador2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/btnCrearJugador2" android:layout_marginTop="20dp"
            app:layout_constraintTop_toBottomOf="@+id/btnEliminar2" app:layout_constraintEnd_toEndOf="@+id/btnEliminar2"
            app:layout_constraintStart_toStartOf="@+id/btnEliminar2"/>
    <Button
            android:text="Button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/btnGestionarJugador2" android:layout_marginTop="24dp"
            app:layout_constraintTop_toBottomOf="@+id/btnCrearJugador2"
            app:layout_constraintEnd_toEndOf="@+id/btnCrearJugador2"
            app:layout_constraintStart_toStartOf="@+id/btnCrearJugador2"/>
</android.support.constraint.ConstraintLayout>